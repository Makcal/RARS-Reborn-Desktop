<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" fillWidth="false" prefHeight="148.0" prefWidth="231.0" spacing="10.0"
      xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="rarsreborn.FileCreationDesignController">
   <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="27.0" prefWidth="288.0" text="New file name:"
          textAlignment="CENTER">
      <font>
         <Font size="18.0"/>
      </font>
   </Label>
   <TextField fx:id="file_name_text_box" prefHeight="39.0" prefWidth="192.0">
      <font>
         <Font size="18.0"/>
      </font>
   </TextField>
   <Button mnemonicParsing="false" onAction="#OnConfirmBtnAction" text="Confirm">
      <font>
         <Font size="18.0"/>
      </font>
   </Button>
</VBox>
